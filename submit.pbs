#!/bin/bash
#PBS -l nodes=1:ppn=16:dc2,walltime=0:20:00
#PBS -N app-splitshells
#PBS -V

cd $PBS_O_WORKDIR

module load matlab


export MATLABPATH=$MATLABPATH:$SCA_SERVICE_DIR
matlab -nodisplay -nosplash -r main

#check for output files
if [ -s dwi.nii.gz ];
then
	echo 0 > finished
else
	echo "output missing"
	echo 1 > finished
	exit 1
fi





